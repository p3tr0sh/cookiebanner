{
  "compilerOptions": {
    "outDir": "./build/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es2017",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "@ress/*": ["res/*"],
    },
  },
  "include": ["./src/**/*.ts", "./res/**/*.json", "./src/**/*.tsx"],
}